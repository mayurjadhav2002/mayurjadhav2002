{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "Tags": {
      "type": "string"
    },
    "comments": {
      "type": "customField",
      "customField": "plugin::comments.comments"
    },
    "tag": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::tag.tag",
      "inversedBy": "blogs"
    },
    "blog": {
      "type": "customField",
      "options": {
        "preset": "toolbarBalloon"
      },
      "customField": "plugin::ckeditor5.CKEditor"
    }
  }
}
